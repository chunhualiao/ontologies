        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/p-plan#"/>
        <rdfs:range rdf:resource="http://purl.org/net/p-plan#Plan"/>
        <rdfs:domain rdf:resource="http://purl.org/net/p-plan#Variable"/>
    </owl:ObjectProperty>
    


